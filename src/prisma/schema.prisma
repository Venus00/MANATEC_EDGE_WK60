datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id                     Int      @id @default(autoincrement())
  version                String?
  version_protocole      String?
  sn                     String?
  total                  String
  unit                   String
  number_weightings      String
  voucher_number         String
  status                 String
  weight_last_stroke     String
  date_last_stroke       String
  time_last_stroke       String
  current_weight_loading String
  created_at             DateTime @default(now())
}

model Alert {
  id          Int      @id @default(autoincrement())
  code        String
  description String
  created_at  DateTime @default(now())
}

model Status {
  id                      Int       @id @default(autoincrement())
  name                    String    @unique
  delta                   Int
  shut                    Int
  total_event             Int
  total_alert             Int
  last_log_date           DateTime?
  last_response_date_vims DateTime?
  engine_status           String
}

model Health {
  id         Int      @id @default(autoincrement())
  health     String
  created_at DateTime @default(now())
}
