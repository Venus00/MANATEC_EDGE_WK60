datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id                     Int      @id @default(autoincrement())
  serial                 String?
  version_protocole      String?
  version                String?
  sn                     String?
  unit                   String?
  total                  String?
  number_weightings      String?
  current_weight_loading String?
  voucher_number         String?
  status                 String?
  weight_last_stroke     String?
  error_message          String?
  error_value            String?
  value                  String?
  date_last_strok        String?
  time_last_stroke       String?
  total_price            String?
  costumer_name          String?
  costumer_number        String?
  material_name          String?
  material_number        String?
  building_name          String?
  building_number        String?
  driver_name            String?
  driver_number          String?
  vehicule_number        String?
  vehicule_name          String?
  container_name         String?
  container_number       String?
  created_at             DateTime @default(now())
}

model Alert {
  id          Int      @id @default(autoincrement())
  code        String?
  serial      String?
  description String?
  created_at  DateTime @default(now())
}

model Status {
  id                     Int       @id @default(autoincrement())
  name                   String    @unique
  delta                  Int
  shut                   Int
  last_log_count_alert   Int?
  last_log_count_payload Int?
  last_log_count_health  Int?
  startup_date           DateTime?
  last_log_date          DateTime?
  last_request_vims      DateTime?
  engine_status          String
}

model Health {
  id         Int      @id @default(autoincrement())
  health     String
  created_at DateTime @default(now())
}
